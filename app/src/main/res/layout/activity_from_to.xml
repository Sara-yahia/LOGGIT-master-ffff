<?xml version="1.0" encoding="utf-8"?>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/background_light"
        tools:context="com.example.sara.loggit.FromToActivity">

    <Button
        android:id="@+id/fromto_ramsis_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/fromto_TahrirSqrBtn_Button"
        android:layout_below="@+id/fromto_ShowMap_Button"
        android:layout_marginTop="62dp"
        android:drawableLeft="@drawable/mapp"
        android:text="Ramsis Square"
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_Abasseya_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fromto_TahrirSqrBtn_Button"
        android:layout_marginStart="29dp"
        android:layout_toEndOf="@+id/fromto_route_TextView"
        android:drawableLeft="@drawable/mapp"
        android:text="Abasseya "
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_TahrirSqrBtn_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fromto_ramsis_Button"
        android:layout_marginTop="16dp"
        android:layout_toStartOf="@+id/fromto_Abasseya_Button"
        android:drawableLeft="@drawable/mapp"
        android:text="Tahrir Square"
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_kobriElKoba_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/fromto_TahrirSqrBtn_Button"
        android:layout_alignBottom="@+id/fromto_TahrirSqrBtn_Button"
        android:layout_alignParentEnd="true"
        android:drawableLeft="@drawable/mapp"
        android:text="Kobri EL-Koba"
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_Esaaf_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/fromto_cairoUnversity_Button"
        android:layout_alignBottom="@+id/fromto_cairoUnversity_Button"
        android:layout_alignStart="@+id/fromto_TahrirSqrBtn_Button"
        android:layout_marginStart="19dp"
        android:drawableLeft="@drawable/mapp"
        android:text="Esaaf"
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_cairoUnversity_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/fromto_kobriElKoba_Button"
        android:layout_below="@+id/fromto_kobriElKoba_Button"
        android:layout_marginTop="14dp"
        android:drawableLeft="@drawable/mapp"
        android:text="Cairo University "
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_ShowMap_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="13dp"
        android:background="@drawable/background_signup"
        android:text="Show Map"
        android:textColor="#FFF"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_giza_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fromto_route_TextView"
        android:layout_alignStart="@+id/fromto_ramsis_Button"
        android:layout_marginBottom="26dp"
        android:drawableLeft="@drawable/mapp"
        android:text="Giza Square"
        android:textColor="#000"
        android:textStyle="bold" />

    <Button
        android:id="@+id/fromto_ShowTrips_Button"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="19dp"
        android:background="@drawable/background_signup"
        android:enabled="false"
        android:text="Show Trips"
        android:textColor="#fff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/fromto_route_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fromto_ShowTrips_Button"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp" />

</RelativeLayout>


